{
  "private": true,
  "scripts": {
    "start": "umi dev",
    "build": "umi build",
    "postinstall": "umi generate tmp",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@ant-design/pro-layout": "^5.0.17",
    "@ant-design/pro-table": "^2.4.1",
    "@tinymce/tinymce-react": "^3.6.0",
    "@umijs/preset-react": "1.x",
    "@umijs/test": "^3.2.9",
    "ahooks": "^2.0.1",
    "cron-parser": "^2.15.0",
    "lint-staged": "^10.0.7",
    "prettier": "^1.19.1",
    "react": "^16.12.0",
    "react-color": "^2.18.1",
    "react-dom": "^16.12.0",
    "umi": "^3.2.9",
    "yorkie": "^2.0.0"
  },
  "devDependencies": {
    "@types/react-color": "^3.0.2",
    "@umijs/preset-ui": "^2.2.1"
  }
}
